			<impuestos>
				<impuesto>
					<property name="codigoImpuesto" join="codigo"/>
					<property name="codigoPorcentajeImpuesto" join="codigoPorcentaje"/>
					<property name="tarifaImpuesto" join="tarifa"/>
					<property name="baseImponibleImpuesto" join="baseImponible"/>
					<property name="valorImpuesto" join="valor"/>
				</impuesto>
			</impuestos>
		</detalle>
	</detalles>
