		<property name="totalDescuentoInfoFactura" join="totalDescuento"/>
		<property name="totalConImpuestosInfoFactura" join="totalConImpuestos"/>
			<totalImpuesto>
				<property name="codigoTotalImpuesto" join="codigo"/>
				<property name="codigoPorcentajeTotalImpuesto" join="codigoPorcentaje"/>
				<property name="baseImponibleTotalImpuesto" join="baseImponible"/>
